import java.util.Scanner;

public class Main {
	
	// Taximeter Program , 3.40, 25 ,14
	
	/* Taksimetre kilometre başına 3.40 tl tutmaktadır ,
	   Kısa mesafe tutarı 25 tl dir , 25 tl altındaki ücretlerde yine 25 tl alınacaktır ,
	   taksimetre açılış ücreti 14 tl dir. */
	 
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Type Km Input:");
		
		double km = scan.nextInt();
		
		
		double price = 14 + (km * 3.40);
		
		if (price <= 25) {
			
			price = 25 ;
			
			System.out.println(price);
			
		}
		
			System.out.println("price's :" + price + "TL");
		
	}

	
}
